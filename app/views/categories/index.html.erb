<h1>Category Page</h1>
<hr>
<br>
<ul class="list-group well">
  <% @categories.each do |category| %>
    <li class="list-group-item"><%= link_to category.name, category_path(category) %></li>
  <% end %>
</ul>
<hr>
<%= link_to "Add a category", new_category_path, :class => "btn btn-success btn-lg"%>
